{
  "manifest_version": 3,
  "name": "GL-ChemDraw Clipboard for FindMolecule",
  "version": "1.0.0",
  "description": "Copy chemical structures from GL-ChemDraw to clipboard for pasting into FindMolecule ELN",
  "key": "",
  "permissions": ["nativeMessaging"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": [
        "https://*.vercel.app/*",
        "https://localhost/*",
        "https://127.0.0.1/*",
        "http://localhost/*",
        "http://127.0.0.1/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
